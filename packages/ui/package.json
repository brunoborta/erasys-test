{
  "name": "@borta/web-ui",
  "version": "1.2.0",
  "description": "Shared UI components for Erasys Test",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brunoborta/erasys-test.git",
    "directory": "packages/ui"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "erasys",
    "ui",
    "components",
    "react",
    "tailwindcss",
    "brunoborta"
  ],
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./globals.css": "./dist/globals.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "react": ">=19",
    "react-dom": ">=19",
    "tailwindcss": "^4.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@tailwindcss/postcss": "^4.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "postcss": "^8.4.35",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.0.0",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3"
  }
}
